{
	"manifest_version": 2,
	"name": "WebsiteLock",
	"version": "1.1",
	"icons": {
		"128": "icon128.png"
	},
	"description": "Puts random password on domains for max productivity",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_title": "WebsiteLock",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"update_url": "https://www.google.com",
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"storage",
		"bookmarks",
		"<all_urls>"
	]
}